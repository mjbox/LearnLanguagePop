{
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "semi": 2,
      "no-unused-vars": "off",
      "no-console": "off",
      "comma-dangle": "off",
      "react/jsx-filename-extension": "off"
    },
    "globals": {
      "document": true
    }
  }